    "navigations": {
      "collection": "navigation",
      "via": "custom_pages",
      "dominant": true
    },
    "hero_banners": {
      "via": "custom_pages",
      "collection": "hero-banners",
      "dominant": true
    },
    "footers": {
      "collection": "footers",
      "via": "custom_pages",
      "dominant": true
    },
    "index": {
      "type": "boolean"
    },
    "follow": {
      "type": "boolean"
    },
    "title": {
      "type": "text"
    },
    "meta": {
      "type": "text"
    },
    "description": {
      "type": "text"
    }
  }
